{
	"name": "@gmahechas/common-utils-ts-be",
	"version": "0.0.0",
	"description": "common-utils-ts-be",
	"author": "gmahechas",
	"license": "ISC",
	"private": false,
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"directories": {
		"lib": "dist"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"eslint": "eslint --config ./node_modules/@gmahechas/common-devtools-ts-be/.eslintrc.js --ignore-path ./node_modules/@gmahechas/common-devtools-ts-be/.eslintignore src/**/*.ts",
		"eslint:fix": "eslint --config ./node_modules/@gmahechas/common-devtools-ts-be/.eslintrc.js --ignore-path ./node_modules/@gmahechas/common-devtools-ts-be/.eslintignore --fix src/**/*.ts",
		"prettier": "prettier --config ./node_modules/@gmahechas/common-devtools-ts-be/.prettierrc --ignore-path ./node_modules/@gmahechas/common-devtools-ts-be/.prettierignore --check \"./src/**/*.{ts,json}\"",
		"prettier:write": "prettier --config ./node_modules/@gmahechas/common-devtools-ts-be/.prettierrc --ignore-path ./node_modules/@gmahechas/common-devtools-ts-be/.prettierignore --check --write \"./src/**/*.{ts,json}\"",
		"lint-staged": "lint-staged --config ./node_modules/@gmahechas/common-devtools-ts-be/.lintstagedrc",
		"commitlint": "commitlint --config ./node_modules/@gmahechas/common-devtools-ts-be/commitlint.config.js --edit",
		"format": "pnpm prettier:write && pnpm eslint:fix",
		"build:development": "rm -rf dist && tsc --project tsconfig.development.json",
		"build:stage": "rm -rf dist && tsc --project tsconfig.stage.json",
		"build:production": "rm -rf dist && tsc --project tsconfig.production.json"
	},
	"dependencies": {
		"uuid": "9.0.1"
	},
	"devDependencies": {
		"@gmahechas/common-devtools-ts-be": "0.0.8",
		"@types/uuid": "9.0.7"
	},
	"peerDependencies": {
		"@types/node": "20.10.4",
		"typescript": "5.3.3"
	}
}
